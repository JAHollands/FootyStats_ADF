{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "football-data-jh-rg"
		},
		"ls_data_lake_footballdatajhsa_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_data_lake_footballdatajhsa'"
		},
		"ls_sql_footballdatajhsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_footballdatajhsql'"
		},
		"ls_data_lake_footballdatajhsa_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://footballdatajhsa.dfs.core.windows.net/"
		},
		"ls_http_Betonjames_DOT_com_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.betonjames.com/wp-content/uploads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "football-data-jh-rg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_data_lake_footballdatajhsa_accountKey": {
						"value": "[parameters('ls_data_lake_footballdatajhsa_accountKey')]"
					},
					"ls_sql_footballdatajhsql_connectionString": {
						"value": "[parameters('ls_sql_footballdatajhsql_connectionString')]"
					},
					"ls_data_lake_footballdatajhsa_properties_typeProperties_url": {
						"value": "[parameters('ls_data_lake_footballdatajhsa_properties_typeProperties_url')]"
					},
					"ls_http_Betonjames_DOT_com_properties_typeProperties_url": {
						"value": "[parameters('ls_http_Betonjames_DOT_com_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}